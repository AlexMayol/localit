{
  "name": "localit",
  "version": "3.0.0",
  "license": "MIT",
  "description": "Simple wrapper class for localStorage and sessionStorage",
  "repository": "alexmayol/localit",
  "main": "dist/localit.js",
  "types": "dist/localit.d.ts",
  "files": [
    "/dist"
  ],
  "author": "Alejandro Mayol",
  "keywords": [
    "HTML5",
    "localStorage",
    "sessionStorage",
    "cache",
    "store",
    "ES6 class",
    "TypeScript"
  ],
  "scripts": {
    "declarations": "tsc -p tsconfig.json",
    "dev": "yarn esbuild src/localit.ts --bundle --outfile=dist/localit.js --watch",
    "build": "yarn declarations && yarn esbuild src/localit.ts --bundle --minify --outfile=dist/localit.js",
    "test": "yarn build && jest"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@types/jest": "^26.0.23",
    "babel-jest": "^26.6.3",
    "esbuild": "^0.11.23",
    "jest": "^26.6.3",
    "regenerator-runtime": "^0.13.7",
    "tsc": "2.0.3",
    "typescript": "^4.2.4"
  }
}
