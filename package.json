{
  "name": "localit",
  "version": "5.1.1",
  "license": "MIT",
  "description": "Simple wrapper class for localStorage and sessionStorage",
  "repository": "alexmayol/localit",
  "main": "./dist/localit.cjs.js",
  "module": "./dist/localit.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/localit.es.js",
      "require": "./dist/localit.cjs.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "Alejandro Mayol",
  "keywords": [
    "HTML5",
    "localStorage",
    "sessionStorage",
    "cache",
    "store",
    "ES6 class",
    "TypeScript"
  ],
  "scripts": {
    "reset": "rimraf dist",
    "build": "vite build",
    "test": "yarn build && vitest test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "pretty": "yarn prettier:fix && yarn lint:fix"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
